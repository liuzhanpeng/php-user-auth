{
    "name": "peng/php-user-auth",
    "description": "通用的php用户认证核心, 方便在各个框架在其之上实现自己的用户认证模块。",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "liuzhanpeng",
        "email": "liuzhanpeng@gmail.com"
    }],
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Lzpeng\\Auth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lzpeng\\Tests\\": "tests/"
        }
    }
}